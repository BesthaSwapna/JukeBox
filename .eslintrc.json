{
  "extends": [
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "no-restricted-syntax": [
      0,
      "ForInStatement",
      "ForOfStatement"
    ],
    "no-console": "off",
    "class-methods-use-this": 0,
    "object-shorthand": [
      "error",
      "always",
      {
        "avoidQuotes": true
      }
    ],
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "radix": 0,
    "func-names": 0,
    "prefer-destructuring": [
      "error",
      {
        "array": false,
        "object": false
      },
      {
        "enforceForRenamedProperties": false
      }
    ]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {}
  },
  "env": {
    "es6": true,
    "node": true
  }
}